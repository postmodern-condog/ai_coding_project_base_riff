{
  "$schema": "See .workstream/README.md for field documentation",
  "project": "my-project",
  "packageManager": "npm",
  "services": {
    "dev": {
      "command": "npm run dev -- --port $WS_PORT_DEV",
      "defaultPort": 3000,
      "healthCheck": "http://localhost:$WS_PORT_DEV"
    }
  },
  "setup": {
    "envFiles": [".env.local"],
    "postInstall": []
  },
  "verify": {
    "steps": ["typecheck", "lint", "test", "build"],
    "commands": {}
  }
}
